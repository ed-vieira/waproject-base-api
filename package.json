{
  "name": "waproject-ebay-server",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "yarn start:dev",
    "start:dev": "yarn ts-node-dev --inspect=0.0.0.0:9229 --respawn --no-notify --transpileOnly --no-deps --project tsconfig.dev.json ./src/index.ts",
    "lint": "yarn eslint \"./src/**/*.ts\"",
    "test": "cross-env NODE_PATH=./src jest --maxWorkers=1",
    "test:watch": "cross-env NODE_PATH=./src jest --maxWorkers=1 --watch",
    "test:cov": "cross-env NODE_PATH=./src jest --maxWorkers=1 --coverage",
    "test:debug": "cross-env NODE_PATH=./src node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_PATH=./src jest --maxWorkers=1 --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js" ,
    "migration:generate": "yarn typeorm migration:generate -d ./src/modules/database/migrations -n",
    "postmigration:generate": "yarn eslint \"./src/modules/database/migrations/**/*.ts\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
  "dependencies": {
    "@nestjs/common": "6.6.7",
    "@nestjs/core": "6.6.7",
    "@nestjs/platform-fastify": "6.6.7",
    "@nestjs/swagger": "3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@sentry/node": "5.6.2",
    "app-module-path": "2.2.0",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.0",
    "date-fns": "2.1.0",
    "date-fns-tz": "1.0.7",
    "fastify-helmet": "3.0.1",
    "fastify-swagger": "2.4.0",
    "jsonwebtoken": "8.5.1",
    "knex": "0.19.3",
    "lodash": "4.17.15",
    "nest-raven": "5.0.0",
    "password-generator": "^2.2.0",
    "pg": "^7.12.1",
    "pino-pretty": "3.2.1",
    "qs": "^6.8.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "source-map-support": "0.5.13",
    "tslib": "1.10.0",
    "typeorm": "^0.2.18",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "6.6.7",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.3",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.4",
    "@types/qs": "6.5.3",
    "@types/supertest": "2.0.8",
    "@types/uuid": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "concurrently": "4.1.2",
    "cross-env": "5.2.1",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "jest-extended": "0.11.2",
    "prettier": "1.18.2",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "ts-node-dev": "1.0.0-pre.32",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.6.2"
  }
}
